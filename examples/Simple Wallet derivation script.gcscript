              }
            },
            {
              "type": "buildAddress",
              "name": "DelegationPortfolioWallet#2",
              "addr": {
                "spendPubKeyHashHex": "{get('cache.HDSpendKeys.0.pubKeyHashHex')}",
                "stakePubKeyHashHex": "{get('cache.HDStakeKeys.2.pubKeyHashHex')}"
              }
            },
            {
              "type": "buildAddress",
