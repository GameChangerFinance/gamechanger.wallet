              "addr": {
                "spendPubKeyHashHex": "{get('cache.HDSpendKeys.5.pubKeyHashHex')}",
                "stakePubKeyHashHex": "{get('cache.HDStakeKeys.0.pubKeyHashHex')}"
              }
            }
          ]
        },
        {
          "type": "script",
          "exportAs": "DelegationPortfolioWallet",
